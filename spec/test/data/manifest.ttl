# Test manifest
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix rdft:   <http://www.w3.org/ns/rdftest#> .

<>  rdf:type mf:Manifest ;
    rdfs:label "Test manifest";
    rdfs:comment "Comment on Test manifest";
    mf:entries (
      <#syntax> <#syntax-negative>
      <#eval> <#eval-negative>
#      <#sparql> <#sparql-negative>
    ) .

<#syntax> a rdft:TestSyntax;
  mf:name "Syntax Test";
  rdfs:comment "Basic Syntax test";
  mf:action <syntax.ttl>;
  .

<#syntax-negative> a rdft:TestNegativeSyntax;
  mf:name "Negative Syntax Test";
  rdfs:comment "Basic Negative Syntax Test";
  mf:action <syntax-negative.ttl>;
  .

<#eval> a rdft:TestEval;
  mf:name "Evaluation Test";
  rdfs:comment "Basic RDF evaluation";
  mf:action <eval.ttl>;
  mf:result <eval.nt>;
  .

<#eval-negative> a rdft:TestNegativeEval;
  mf:name "Negative Evaluation Test";
  rdfs:comment "Basic RDF negative evaluation";
  mf:action <eval-negative.ttl>;
  mf:result <eval-negative.nt>;
  .

#<#sparql> <#sparql-negative>
